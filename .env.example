# =============================================================================
# Ollama Configuration (Optional - for local LLM)
# =============================================================================
VITE_OLLAMA_BASE_URL=http://localhost:11434
VITE_OLLAMA_MODEL=llama3.1

# =============================================================================
# Firebase Configuration
# =============================================================================
# Get these from your Firebase Console
# https://console.firebase.google.com
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# =============================================================================
# Google Gemini AI Configuration
# =============================================================================
# Get API key from: https://makersuite.google.com/app/apikey
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# =============================================================================
# License Configuration (For Production - Optional)
# =============================================================================
# VITE_LICENSE_KEY=your_license_key_here
# Contact: etherjoon@gmail.com for commercial licensing

# =============================================================================
# Application Settings
# =============================================================================
VITE_APP_NAME=Resumate
VITE_APP_VERSION=1.0.0

